{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "noEmit": false,
    "outDir": "./dist",
    "esModuleInterop": true,
    "module": "ESNext",
    "rootDir": "./src",
    "forceConsistentCasingInFileNames": false,
  },
  "include": [
    "./src/**/*.ts",
    "!./src/**/*.d.ts"
  ],
  "exclude": [
    "./src/**/*.spec.ts"
  ]
}