    ".": "./dist/index.js",
    "./dev": "./dist/dev/index.js"
  },
  "types": "./src/index.ts",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sushruth/csar.git"
  },
  "scripts": {
