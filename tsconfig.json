{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "strict": true,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "module": "es6",
        "moduleResolution": "node",
        "target": "es5",
        "allowJs": true,
        "jsx": "react",
        "lib": ["esnext", "dom"],
        "downlevelIteration": true,
        "strictNullChecks": true
    },
    "include": [
        "./src/**/*"
, "src/create-state/.types.ts"    ]
}